{
    "name": "projnotes",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "cross-env NODE_ENV=production node ./dist-server/bin/www",
        "w:d": "cross-env NODE_ENV=developmen babel-watch --watch ./server/views ./server/bin/www",
        "w:s": "cross-env NODE_ENV=development webpack server --config ./webpack.dev.config.js --mode development",
        "cleans": "rimraf dist-server",
        "cleanc": "rimraf public/js",
        "build-dev": "webpack",
        "b:s": "npm run cleans && babel ./server --out-dir dist-server --copy-files",
        "b:c": "npm run cleanc && webpack --config ./webpack.config.js --mode production",
        "build": "npm run b:c && npm run b:s"
    },
    "dependencies": {
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "express": "~4.16.1",
        "hbs": "~4.1.1",
        "http-errors": "~1.6.3",
        "morgan": "~1.9.1",
        "rimraf": "^3.0.2",
        "webpack": "^5.35.0",
        "webpack-dev-middleware": "^4.2.0",
        "webpack-hot-middleware": "^2.25.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.13.14",
        "@babel/core": "^7.13.15",
        "@babel/node": "^7.13.13",
        "@babel/preset-env": "^7.13.15",
        "@webpack-cli/generators": "^2.3.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-module-resolver": "^4.1.0",
        "babel-preset-minify": "^0.5.1",
        "babel-watch": "^7.4.1",
        "core-js": "^3.11.1",
        "cross-env": "^7.0.3",
        "css-loader": "^5.2.4",
        "eslint": "^7.26.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-import": "^2.23.2",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-webpack-plugin": "^2.5.4",
        "mini-css-extract-plugin": "^2.4.2",
        "webpack-cli": "^4.6.0",
        "webpack-dev-server": "^3.11.2"      
    }
}
